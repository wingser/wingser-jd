Declare Function GetAsyncKeyState Lib "user32" Alias "GetAsyncKeyState" (ByVal vKey As Long) As Integer
Declare Function GetKeyState Lib"user32"(ByVal nVirtKey As Long) As Integer
UserVar 开启连击=DropList{"开启":"1"|"关闭":"0"}=0 "连击"
UserVar autoA="O" "连击A"
UserVar autoB="P" "连击B"
UserVar 连击间隔=DropList{"每秒3次":"320"|"每秒5次":"190"|"每秒10次":"90"}=1 "选择连击速度"









Do
    //自定义设置
    If 开启连击 = 1 Then 
        '点击按键
        If GetAsyncKeyState(Asc(autoA)) < 0 or GetAsyncKeyState(Asc(autoA)) = 1 or GetAsyncKeyState(Asc(autoA)) > 32767 Then 
            KeyPress "U", 1
            Delay 连击间隔
        End If
        If GetAsyncKeyState(Asc(autoB)) < 0 or GetAsyncKeyState(Asc(autoB)) = 1 or GetAsyncKeyState(Asc(autoB)) > 32767 Then 
            KeyPress "I", 1
            Delay 连击间隔
        End If
    End If


    '按下Q键检测（等于同事按下AS）
    If GetAsyncKeyState(81) < 0 or GetAsyncKeyState(81) = 1 or GetAsyncKeyState(81) > 32767 Then 
        Call lib.键盘.键盘按键组("A,S",0,40)// AS按键间隔时间为2毫秒，时间误差5毫秒之内。
    End If
    '按下e键检测（等于同事按下SD）
    If GetAsyncKeyState(69) < 0 or GetAsyncKeyState(69) = 1 or GetAsyncKeyState(69) > 32767 Then 
        Call lib.键盘.键盘按键组("D,S",0,40)// AS按键间隔时间为2毫秒，时间误差5毫秒之内。
    End If
    Delay 10
Loop

